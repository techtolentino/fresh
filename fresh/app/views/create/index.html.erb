<div class="row">
	<div class="large-12 mid-screen columns text-center">
		<div class="large-6 columns text-center border">
			<h4 class="bg-purple white-text">PREVIEW</h4>
			<div class="large-12 columns">
				<img class="tshirt" id="shirt_color" src="t-white.png">
				<img class="hidden" id="egg_shirt" src="t-black.png">
			</div>
		</div>
		<div class="large-6 columns text-center border">
			<h4 class="bg-purple white-text">SWAG OUT</h4>
			<div class="large-12 columns">
				<br>
				<br>
				<label><h5>Symbolize</h5>
	        <select>
	          <option value="{ }">Hash</option>
						<option value="<%  %>">View</option>
						<option value="[ ]">Array</option>
						<option value="=>">Hash Rocket</option>
						<option value="< />">Tag</option>
						<option value="&&||">And Or</option>
	        </select>
      	</label>
      	<label><h5>Phrasify</h5>
	        <select>
	          <option value="Undefined Method">Undefined Method</option>
						<option value="!false">true</option>
						<option value="unexpected token">unexpected token</option>
						<option value="def initialize">.new</option>
						<option value="git -force">Git Force</option>
						<option value="rake">RAKE UP</option>
						<option value="rspec">RSpec My Shit</option>
	        </select>
      	</label>
      	<label><h5>Colorize</h5>
	        <select>
		        <option value="#1abc9c">Turquoise</option>
						<option value="#27ae60">Nephritis</option>
						<option value="#9b59b6">Amethyst</option>
						<option value="#34495e">Asphalt</option>
						<option value="#e74c3c">Alizarin</option>
						<option value="#7f8c8d">Concrete</option>
						<option value="#f39c12">Orange</option>
	        </select>
      	</label>
				<br>
				<a href="#" class="button smaller" id="create_button">CREATE THAT</a>
				<br>
				<a href="#" class="button smaller" id="shirt_button">SHIRT COLOR</a>
			</div>
		</div>
	</div>
</div>